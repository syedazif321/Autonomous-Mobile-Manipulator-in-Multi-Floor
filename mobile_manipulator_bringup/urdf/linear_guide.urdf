<?xml version="1.0"?>
<robot name="linear_guide" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- ============ Base ============ -->
  <link name="base_link">
    <visual>
      <geometry>
        <mesh filename="file:///home/azif/projetcs/Autonomous-Mobile-Manipulator-in-Multi-Floor/mobile_manipulator_bringup/meshes/base.dae"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="file:///home/azif/projetcs/Autonomous-Mobile-Manipulator-in-Multi-Floor/mobile_manipulator_bringup/meshes/base.dae"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="10"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1" iyy="1" izz="1" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <!-- ============ Vertical Column ============ -->
  <link name="vertical_column_link">
    <visual>
      <geometry>
        <mesh filename="file:///home/azif/projetcs/Autonomous-Mobile-Manipulator-in-Multi-Floor/mobile_manipulator_bringup/meshes/vertical_column.dae"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="file:///home/azif/projetcs/Autonomous-Mobile-Manipulator-in-Multi-Floor/mobile_manipulator_bringup/meshes/vertical_column.dae"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="5"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.5" iyy="0.5" izz="0.5" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <!-- ============ Prismatic Joint ============ -->
  <joint name="base_to_column_joint" type="prismatic">
    <parent link="base_link"/>
    <child link="vertical_column_link"/>
    <origin xyz="0 0 0.1"/>
    <axis xyz="0 0 1"/>
    <limit lower="0.0" upper="1.0" effort="1000" velocity="0.5"/>
    <dynamics damping="10.0" friction="1.0"/>
  </joint>

  <!-- ============ ros2_control Plugin ============ -->
  <gazebo>
    <plugin name="gazebo_ros2_control" filename="libgazebo_ros2_control.so">
      <robot_sim_type>gazebo_ros2_control/DefaultRobotHWSim</robot_sim_type>
    </plugin>
  </gazebo>

