          <uri>model://f1_dummy_harmonic</uri>
          <name>f1_dummy_harmonic_6</name>
          <pose>-350.8 -290.2 0 0 0 -1.33</pose>
        </include>

        <include>
          <uri>model://f1_dummy_harmonic</uri>
