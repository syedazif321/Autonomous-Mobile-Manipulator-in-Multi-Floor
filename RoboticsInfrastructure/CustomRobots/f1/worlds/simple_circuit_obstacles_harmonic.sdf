        </include>

        <include>
          <uri>model://f1_dummy_harmonic</uri>
          <name>f1_dummy_harmonic_9</name>
          <pose>-166.75 160.3 0 0 0 0</pose>
        </include>

        <include>
          <uri>model://f1_dummy_harmonic</uri>
          <name>f1_dummy_harmonic_10</name>
          <pose>-154 105 0 0 0 0.785</pose>
        </include>

        <include>
          <uri>model://f1_dummy_harmonic</uri>
          <name>f1_dummy_harmonic_11</name>
