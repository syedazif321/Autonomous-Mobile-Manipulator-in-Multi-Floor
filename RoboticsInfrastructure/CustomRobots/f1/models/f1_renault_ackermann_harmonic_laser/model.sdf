<?xml version="1.0"?>
<sdf version="1.6">

  <model name="f1_renault_ackermann_harmonic_laser">
    <pose>0 0 0.0 0 0 0</pose>

    <link name='base_link'>
      <inertial>
        <mass>0.1</mass>
        <inertia>
          <ixx>0.001</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.001</iyy>
          <iyz>0</iyz>
          <izz>0.001</izz>
        </inertia>
      </inertial>
