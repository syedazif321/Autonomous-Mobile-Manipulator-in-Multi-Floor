        <pose>0.875 0.1444 1.5174 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>0.1 0.1 0.1</scale>
            <uri>model://f1_renault_ackermann_harmonic_laser/Renault/front_left.obj</uri>
          </mesh>
