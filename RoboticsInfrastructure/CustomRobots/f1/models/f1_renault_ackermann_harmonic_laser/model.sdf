        <pose>0 1.51 0 1.5708 -0 1.5708</pose>
        <geometry>
          <mesh>
            <scale>5 5 5</scale>
            <uri>model://f1_renault_ackermann_harmonic_laser/Renault/chasis.obj</uri>
