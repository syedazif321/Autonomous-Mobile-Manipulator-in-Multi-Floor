    <!-- Single Steering Joint for both front wheels -->
    <joint name='front_steering' type='revolute'>
      <pose relative_to='chassis_link'>0 1.51736 0.324997 0 -0 0</pose>
      <parent>chassis_link</parent>
      <child>front_steering_link</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-0.3</lower>
          <upper>0.3</upper>
          <effort>1000</effort>
          <velocity>2</velocity>
        </limit>
        <dynamics>
          <friction>0.5</friction>
          <damping>0.1</damping>
        </dynamics>
      </axis>
    </joint>
    <link name='front_steering_link'>
      <pose relative_to='front_steering'>0 0 0 0 -0 0</pose>
      <inertial>
        <pose>0 0 0 0 -0 0</pose>
        <mass>2</mass>
        <inertia>
          <ixx>0.01</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.01</iyy>
          <iyz>0</iyz>
          <izz>0.01</izz>
        </inertia>
      </inertial>
      <self_collide>0</self_collide>
