          <friction>
            <ode>
              <mu>1.7</mu>
              <mu2>1.7</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name='fr_1_visual'>
        <!-- <pose>-0.875 -1.5174 0.1444 0 -0 0</pose> -->
        <pose>-0.875 0.1444 1.5174 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>0.1 0.1 0.1</scale>
            <uri>model://f1_renault_ackermann_harmonic_laser/Renault/front_right.obj</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
    
    <link name='laser_body'>
      <pose relative_to='chassis_link'>0.00000 1.700000 0.7000 0.000000 0.000000 1.57000</pose>
      <inertial>
        <mass>0.1</mass>
        <inertia>
          <ixx>0.001</ixx>
          <ixy>0.0</ixy>
          <iyy>0.001</iyy>
          <ixz>0.0</ixz>
          <iyz>0.0</iyz>
          <izz>0.001</izz>
        </inertia>
      </inertial>
      <visual name="visual_laser">
        <geometry>
          <mesh>
            <uri>model://f1_renault_ackermann_harmonic_laser/meshes/hokuyo.dae</uri>
          </mesh>
        </geometry>
      </visual>
      <sensor name='laser' type='gpu_ray'>
        <pose>0 0 0 0 0 0</pose>
        <ray>
          <scan>
            <horizontal>
              <samples>180</samples>
              <resolution>1.000000</resolution>
              <min_angle>-1.570000</min_angle>
              <max_angle>1.570000</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.080000</min>
            <max>10.000000</max>
            <resolution>0.010000</resolution>
          </range>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.01</stddev>
          </noise>
        </ray>
        <update_rate>20.000000</update_rate>
        <always_on>true</always_on>
        <visualize>true</visualize>
        <topic>/model/f1_renault_ackermann_harmonic_laser/laser/scan</topic>
      </sensor>
      <self_collide>false</self_collide>
    </link>
    
    <joint type="fixed" name="laser_fix">
      <pose>0 0 0 0 0 0</pose>
      <child>laser_body</child>
      <parent>chassis_link</parent>
      <axis>
        <xyz>0 0 1</xyz>
      </axis>
    </joint>

    <!-- Gazebo Harmonic Plugins -->
    
    <!-- Ackermann Steering Plugin -->
    <plugin filename="gz-sim-ackermann-steering-system" name="gz::sim::systems::AckermannSteering">
      <left_joint>fl_axle</left_joint>
      <left_joint>bl_axle</left_joint>
      <right_joint>fr_axle</right_joint>
      <right_joint>br_axle</right_joint>
      <left_steering_joint>front_steering</left_steering_joint>
      <right_steering_joint>front_steering</right_steering_joint>
      <kingpin_width>1.65</kingpin_width>
      <steering_limit>0.3</steering_limit>
      <wheel_base>3.351</wheel_base>
      <wheel_separation>1.65</wheel_separation>
      <wheel_radius>0.3425</wheel_radius>
      <sub_topic>/model/f1_renault_ackermann_harmonic_laser/steer_angle</sub_topic>
      <min_velocity>-50</min_velocity>
      <max_velocity>50</max_velocity>
      <min_acceleration>-50</min_acceleration>
      <max_acceleration>50</max_acceleration>
      <topic>/model/f1_renault_ackermann_harmonic_laser/cmd_vel</topic>
    </plugin>

