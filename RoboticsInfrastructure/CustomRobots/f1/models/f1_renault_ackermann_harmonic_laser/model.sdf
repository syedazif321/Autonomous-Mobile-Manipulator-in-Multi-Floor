        </geometry>
        <surface>
          <contact>
            <ode />
          </contact>
          <friction>
            <ode>
              <mu>1.7</mu>
              <mu2>1.7</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name='br_1_visual'>
        <!-- <pose>-0.875 1.0136 0.1444 0 -0 0</pose> -->
        <pose>-0.875 0.14 -1.0126 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>0.1 0.1 0.1</scale>
            <uri>model://f1_renault_ackermann_harmonic_laser/Renault/back_right.obj</uri>
          </mesh>
        </geometry>
      </visual>
      <self_collide>0</self_collide>
    </link> 


    <!-- Steering Wheel -->
    <joint name="steering_joint" type="revolute">
      <pose relative_to='chassis_link'>-0.01 0.6 0.6 0 0 0</pose>
      <parent>chassis_link</parent>
      <child>steering_wheel</child>
      <axis>
        <xyz>0.0 -1.0 0.0</xyz>
        <limit>
          <lower>-5</lower>
          <upper>5</upper>
          <effort>100</effort>
          <velocity>10</velocity>
        </limit>
        <dynamics>
          <damping>10.0</damping>
        </dynamics>
        <use_parent_model_frame>true</use_parent_model_frame>
      </axis>
      <physics>
        <ode>
          <cfm_damping>1</cfm_damping>
        </ode>
      </physics>
    </joint>
    <link name='steering_wheel'>
      <pose relative_to='steering_joint'>0 0.87 -0.54 0 0 1.5708</pose>
      <inertial>
        <mass>0.5</mass>
        <inertia>
          <ixx>0.01</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.01</iyy>
          <iyz>0</iyz>
          <izz>0.01</izz>
        </inertia>
      </inertial>
      <visual name='steering_wheel_visual'>
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>4.7 4.7 4.7</scale>
            <uri>model://f1_renault_ackermann_harmonic_laser/Renault/steering_wheel.dae</uri>
          </mesh>
        </geometry>
      </visual>
      <self_collide>1</self_collide>
    </link>

    
