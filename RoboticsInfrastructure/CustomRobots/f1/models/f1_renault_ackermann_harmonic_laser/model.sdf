    <!-- Front Right Wheel -->
    <joint name='fr_axle' type='revolute'>
      <pose relative_to='front_steering_link'>0.825 0 0 0 -0 0</pose>
      <parent>front_steering_link</parent>
      <child>fr_1</child>
      <axis>
        <xyz>-1 0 0</xyz>
        <limit>
          <effort>1000</effort>
          <velocity>100</velocity>
        </limit>
        <dynamics>
          <friction>12.031606150200002</friction>
