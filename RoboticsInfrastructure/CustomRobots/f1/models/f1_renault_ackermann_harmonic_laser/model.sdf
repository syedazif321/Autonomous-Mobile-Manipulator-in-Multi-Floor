        <pose>-0.875 -1.5174 0.1444 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>0.001 0.001 0.001</scale>
            <uri>model://f1_renault_ackermann_harmonic_laser/Renault/fr_1.stl</uri>
