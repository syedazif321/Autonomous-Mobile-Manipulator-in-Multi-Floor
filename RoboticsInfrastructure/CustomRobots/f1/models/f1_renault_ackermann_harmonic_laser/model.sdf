      <inertial>
        <pose>-0.0377 0 -0 0 -0 0</pose>
        <mass>24.5</mass>
        <inertia>
          <ixx>1.84514</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1.04961</iyy>
          <iyz>0</iyz>
          <izz>1.04961</izz>
