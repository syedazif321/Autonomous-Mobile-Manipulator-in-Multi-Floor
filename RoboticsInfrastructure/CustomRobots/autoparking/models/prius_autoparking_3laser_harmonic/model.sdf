            <stddev>0.01</stddev>
          </noise>
        </ray>
      </sensor>
    
    <sensor name="side_hokuyo_sensor" type="gpu_ray">
    <pose relative_to='base_link'>1.100000 0.000000 0.500000 0.000000 0.000000 0.000000</pose>
        <always_on>true</always_on>
