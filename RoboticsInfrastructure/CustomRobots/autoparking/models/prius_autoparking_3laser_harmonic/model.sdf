    <!--STEERING WHEEL-->
    <joint name="steering_joint" type="revolute">
      <pose relative_to='chassis_link'>-0.01 0.6 0.6 0 0 0</pose>
      <parent>chassis_link</parent>
      <child>steering_wheel</child>
      <axis>
        <xyz>0.0 -1.0 0.0</xyz>
        <limit>
          <lower>-100</lower>
          <upper>100</upper>
        </limit>
        <dynamics>
          <damping>10.0</damping>
        </dynamics>
      </axis>
    </joint>
    <link name='steering_wheel'>
      <pose relative_to='steering_joint'>0 0.87 -0.54 0 0 1.5708</pose>
    </link>

    <!-- Gazebo Harmonic Ackermann Steering Plugin -->
    <plugin filename="gz-sim-ackermann-steering-system" name="gz::sim::systems::AckermannSteering">
      <left_joint>fl_axle</left_joint>
      <left_joint>bl_axle</left_joint>
      <right_joint>fr_axle</right_joint>
      <right_joint>br_axle</right_joint>
      <left_steering_joint>l_steer</left_steering_joint>
      <right_steering_joint>r_steer</right_steering_joint>
      <kingpin_width>1.65</kingpin_width>
      <steering_limit>0.52</steering_limit>
      <wheel_base>3.351</wheel_base>
      <wheel_separation>1.65</wheel_separation>
      <wheel_radius>0.3425</wheel_radius>
      <min_velocity>-50</min_velocity>
      <max_velocity>50</max_velocity>
      <min_acceleration>-50</min_acceleration>
      <max_acceleration>50</max_acceleration>
      <topic>/prius_autoparking/cmd_vel</topic>
    </plugin>

    <!-- Joint State Publisher -->
    <plugin filename="gz-sim-joint-state-publisher-system" name="gz::sim::systems::JointStatePublisher">
      <joint_name>fl_axle</joint_name>
      <joint_name>fr_axle</joint_name>
      <joint_name>bl_axle</joint_name>
      <joint_name>br_axle</joint_name>
      <joint_name>l_steer</joint_name>
      <joint_name>r_steer</joint_name>
      <joint_name>steering_joint</joint_name>
    </plugin>

    <!-- Odometry Publisher -->
    <plugin filename="gz-sim-odometry-publisher-system" name="gz::sim::systems::OdometryPublisher">
      <odom_topic>/prius_autoparking/odom</odom_topic>
      <robot_base_frame>chassis_link</robot_base_frame>
      <odom_frame>odom</odom_frame>
      <odom_publish_frequency>50</odom_publish_frequency>
      <dimensions>3</dimensions>
    </plugin>
    
    <!-- Pose Publisher -->
    <plugin filename="gz-sim-pose-publisher-system" name="gz::sim::systems::PosePublisher">
      <publish_link_pose>true</publish_link_pose>
      <publish_collision_pose>false</publish_collision_pose>
      <publish_visual_pose>false</publish_visual_pose>
      <publish_nested_model_pose>true</publish_nested_model_pose>
      <update_frequency>50</update_frequency>
    </plugin>
    
